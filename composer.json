{
    "name": "hieu-le/active",
    "description": "The helper class for Laravel applications to get active class base on current route",
    "keywords": [
        "laravel",
        "active",
        "routing"
    ],
    "license": "MIT",
    "homepage": "https://www.hieule.info/tag/laravel-active/",
    "authors": [
        {
            "name": "Hieu Le",
            "email": "letrunghieu.cse09@gmail.com",
            "homepage": "https://www.hieule.info"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "HieuLe\\Active\\": "src/",
            "HieuLe\\ActiveTest\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "HieuLe\\Active\\ActiveServiceProvider"
            ],
            "aliases": {
                "Active": "HieuLe\\Active\\Facades\\Active"
            }
        }
    }
}
